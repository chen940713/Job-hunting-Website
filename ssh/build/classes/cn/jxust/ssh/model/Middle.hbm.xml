<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
   
<hibernate-mapping>
	<class name="cn.jxust.ssh.model.Middle" table="middle">
		<id name="middle_id" column="middle_id" type="java.lang.Integer">
			<generator class="native"></generator>
		</id>
		
		<property name="customerName" column="customerName" length="10" type="java.lang.String"></property>
		<property name="jobinfoId" column="jobinfoId" length="10" type="java.lang.Integer"></property>
		<property name="companyId" column="companyId" length="10" type="java.lang.Integer"></property>
		
	</class>
</hibernate-mapping>